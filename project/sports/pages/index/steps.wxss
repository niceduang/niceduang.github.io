/* pages/index/steps.wxss */
@import "public.wxss";
.steps-head{
	position: relative;
	background: #ff6c00 url("data:image/jpeg;base64,") 50% 100% no-repeat;
	background-size: 100% auto;
	padding: 40rpx 0 124rpx;
}
.steps-head-txt{
	text-align: center;
	color: #ffffff;
	line-height: 1.4em;
	padding-bottom: 5rpx;
}
.steps-head-tit{
	font-size: 36rpx;
}
.steps-head-num{
	font-size: 24rpx;
}
.steps-head-num text{
	padding: 0 12rpx;
}
.steps-line{
	position: relative;
	/* width: 670rpx; */
	/* height: 250rpx; */
  width: 335px;
	height: 125px;
	margin: 0 auto;
	background-color: rgba(255,255,255,0.25);
}
.canvas{
  width: 100%;
  height: 100%;
}
.steps-cont{
	position: relative;
	width: 675rpx;
	margin: 0 auto;
	flex: 1;
	display: flex;
	flex-flow: column nowrap;
	justify-content: flex-start;
}
.steps-cont-tit{
	position: relative;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	font-size: 32rpx;
	color: #ff6c00;
	border-bottom: 1px solid #ffd1b0;
}
.steps-cont-scroll{
	flex: 1;
	overflow-x: hidden;
	overflow-y: scroll;
	-webkit-overflow-scrolling:touch;
}
.steps-scroll-row{
	height: 100rpx;
	border-bottom: 1px solid #efefef;
	display: flex;
	flex-flow: row nowrap;
	justify-content: flex-start;
	align-items: center;
	font-size: 28rpx;
	color: #353535;
}
.steps-scroll-row > view{
	box-sizing: border-box;
}
.steps-scroll-row > view:nth-of-type(1){
	width: 30rpx;
	color: #ff6c00;
}
.steps-scroll-row > view:nth-of-type(2){
	width: 450rpx;
}
.steps-scroll-row > view:nth-of-type(3){
	flex: 1;
	text-align: right;
	padding: 0 10rpx;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.steps-user-info{
	margin-left: 25rpx;
	display: flex;
	flex-flow: row nowrap;
	justify-content: flex-start;
	align-items: center;
}
.steps-user-img{
	position: relative;
	margin-right: 20rpx;
	width: 60rpx;
	height: 60rpx;
	overflow: hidden;
	border-radius: 50%;
}
.steps-user-img image{
	display: block;
	width: 100%;
	height: 100%;
	border-radius: 50%;
}
.steps-user-name{
	flex: 1;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
